<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="fetch">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import fetch urdf file -->
    <xacro:include filename="$(find fetch)/urdf/fetch.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="fetch.ros2_control.xacro" />


    <xacro:fetch_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
